{
  "name": "fast-ttl-cache",
  "version": "0.1.4",
  "description": "ttl cache with capacity support use no timer",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "tsup src/ --clean --format cjs,esm --dts --watch",
    "build": "tsup src/ --clean --format cjs,esm --dts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xincici/fast-ttl-cache"
  },
  "author": "linye<llxy8687@foxmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "ts-jest": "^29.4.3",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0"
  }
}
